{
    "cubes":  [
      {
          "name": "spending",
          "name": "spending",
          "measures": [
            { "name": "award_amount" ,"label": "Award amount"}
          ],
          "aggregates":  [
            {
                "name": "award_sum",
                "label": "Total Award Amount",
                "function": "sum",
                "measure": "award_amount"
            },
            {
                "name": "transaction_count",
                "function": "count"
            }
          ],
          "dimensions":  [
                            "date",
                            "place",
                            "product",
                            "recipient",
                            "agency",
                            "transaction_type"
                         ],
          "joins":  [
              {
                  "master": "date_id",
                  "detail": "dim_date.id"
              },
              {
                  "master": "place_id",
                  "detail": "dim_place.id"
              },
              {
                  "master": "product_id",
                  "detail": "dim_product.id"
              },
              {
                  "master": "recipient_id",
                  "detail": "dim_recipient.id"
              },
              {
                  "master": "agency_id",
                  "detail": "dim_agency.id"
              },
              {
                  "master": "transaction_type_id",
                  "detail": "dim_transaction_type.id"
              }
          ]
      }
    ],
    "dimensions":  {
        "date": {
            "name": "date",
            "label": "Date",
            "role": "time",

            "levels":  [
                {
                    "name": "full_date",
                    "label": "YYYY-MM-DD",
                    "attributes":  [
                        { "name": "full_date" }
                    ]
                },
                {
                    "name": "year",
                    "label": "Year",
                    "attributes":  [
                        { "name": "year" }
                    ]
                },
                {
                    "name": "month",
                    "label": "Month",
                    "attributes":  [
                        { "name": "month" },
                        { "name": "month_name" }
                    ]
                },
                {
                    "name": "day_year",
                    "label": "Day in the year",
                    "attributes":  [
                        { "name": "day_year" }
                    ]
                },
                {
                    "name": "day",
                    "label": "Day",
                    "attributes": [
                        { "name": "day" }
                    ]
                },
                {
                    "name": "weekday_name",
                    "label": "Day of the Week",
                    "attributes":  [
                        { "name": "weekday_name" }
                    ]
                },
                {
                    "name": "calendar_week",
                    "label": "Week in the year",
                    "attributes":  [
                        { "name": "calendar_week"}
                    ]
                },
                {
                    "name": "quarter",
                    "label": "QUarter",
                    "attributes":  [
                        { "name": "quarter" }
                    ]
                }
            ],
            "hierarchies":  [
                {
                    "name": "ymd",
                    "levels": ["year", "month", "day"]
                },
                {
                    "name": "yqmd",
                    "levels": ["year", "quarter", "month", "day"]
                },
                {
                    "name": "ym",
                    "levels": ["year", "month"]
                }
            ]
        }
    }
}
